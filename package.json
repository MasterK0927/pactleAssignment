{
  "name": "pactle-quote-generator",
  "version": "1.0.0",
  "description": "Automated quote generation system for manufacturers",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "db:migrate": "node dist/scripts/run-migrations.js",
    "db:seed": "node dist/scripts/seed-database.js",
    "db:migrate:dev": "ts-node scripts/run-migrations.ts",
    "db:seed:dev": "ts-node scripts/seed-database.ts",
    "db:setup": "npm run db:migrate && npm run db:seed"
  },
  "keywords": [
    "quote",
    "rfq",
    "manufacturing",
    "automation"
  ],
  "author": "Pactle",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/supertest": "^2.0.0",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "supertest": "^6.3.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/crypto-js": "^4.2.2",
    "@types/fast-levenshtein": "^0.0.4",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/papaparse": "^5.3.16",
    "@types/pg": "^8.15.5",
    "@types/puppeteer": "^5.4.7",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "csv-parser": "^3.2.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.0.0",
    "express": "^4.18.0",
    "fast-levenshtein": "^3.0.0",
    "jspdf": "^2.5.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "papaparse": "^5.5.3",
    "pdfkit": "^0.15.0",
    "@types/pdfkit": "^0.13.4",
    "pg": "^8.16.3",
    "puppeteer": "^24.21.0",
    "stripe": "^18.5.0",
    "uuid": "^9.0.0",
    "winston": "^3.8.0"
  }
}
